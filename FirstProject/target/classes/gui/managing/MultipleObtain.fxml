<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainPane" prefHeight="134.0" prefWidth="282.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.managing.MultipleObtainController">
   <children>
      <Text layoutX="12.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #CFCFCF;" text="Obtaining account :" />
      <ChoiceBox fx:id="accountChoice" layoutX="119.0" layoutY="6.0" prefWidth="150.0" />
      <Text layoutX="15.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #CFCFCF;" text="Amount obtained :" />
      <TextField fx:id="amountText" layoutX="118.0" layoutY="38.0" prefHeight="25.0" prefWidth="150.0" promptText="enter the amount..." />
      <Text layoutX="14.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #CFCFCF;" text="Reason :" />
      <TextField fx:id="reasonText" layoutX="65.0" layoutY="70.0" prefHeight="25.0" prefWidth="204.0" promptText="enter the reason..." />
      <Button fx:id="validate" defaultButton="true" layoutX="73.0" layoutY="104.0" mnemonicParsing="false" onAction="#Validate" text="Validate" />
      <Button fx:id="cancel" cancelButton="true" layoutX="152.0" layoutY="104.0" mnemonicParsing="false" onAction="#exitButtonOnAction" text="Cancel" />
   </children>
</AnchorPane>
