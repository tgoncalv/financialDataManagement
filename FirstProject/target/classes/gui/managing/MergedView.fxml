<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.managing.MergedViewController">
   <children>
      <ListView fx:id="accountsList" focusTraversable="false" layoutX="21.0" layoutY="102.0" onMouseClicked="#unselect" prefHeight="260.0" prefWidth="358.0" />
      <Text layoutX="21.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Money type : " />
      <ChoiceBox fx:id="moneyTypeChoiceBox" layoutX="103.0" layoutY="5.0" prefWidth="150.0" />
      <Text layoutX="21.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Account type :" />
      <ChoiceBox fx:id="accountTypeChoiceBox" layoutX="103.0" layoutY="36.0" prefWidth="150.0" />
      <Button fx:id="selectAllButton" layoutX="21.0" layoutY="69.0" mnemonicParsing="false" onAction="#selectAll" text="Select all" />
      <Button fx:id="unselectAllButton" layoutX="93.0" layoutY="69.0" mnemonicParsing="false" onAction="#unselectAll" text="Unselect all" />
      <Button fx:id="validateButton" defaultButton="true" layoutX="131.0" layoutY="369.0" mnemonicParsing="false" onAction="#validate" text="Validate" />
      <Button fx:id="exitButton" cancelButton="true" layoutX="210.0" layoutY="369.0" mnemonicParsing="false" onAction="#exitButtonOnAction" text="Cancel" />
   </children>
</AnchorPane>
