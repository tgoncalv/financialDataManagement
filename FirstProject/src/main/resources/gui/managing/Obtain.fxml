<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainPane" prefHeight="102.0" prefWidth="282.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.managing.ObtainController">
   <children>
      <Text layoutX="12.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #CFCFCF;" text="Amount received :" />
      <TextField fx:id="amountText" layoutX="112.0" layoutY="6.0" prefHeight="25.0" prefWidth="155.0" promptText="enter the amount..." />
      <Text layoutX="12.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #CFCFCF;" text="Reason :" />
      <TextField fx:id="reasonText" layoutX="64.0" layoutY="38.0" prefHeight="25.0" prefWidth="204.0" promptText="enter the reason..." />
      <Button fx:id="validate" defaultButton="true" layoutX="72.0" layoutY="72.0" mnemonicParsing="false" onAction="#Validate" text="Validate" />
      <Button fx:id="cancel" cancelButton="true" layoutX="151.0" layoutY="72.0" mnemonicParsing="false" onAction="#exitButtonOnAction" text="Cancel" />
   </children>
</AnchorPane>
